<%- include('_partial/_header'); -%>


<div class="col-md-8">
    <div class="panel panel-default">
 
        <div class="panel-heading">
            <h3>Crypto Transfer</h3>
        </div>
        <div class="panel-body">
            <form class="form-signin" method="POST" action="/crypto">
                <div class="input-group">
                    <span class="input-group-addon" id="basic-addon1">id</span>
                    <input type="text" class="form-control" placeholder="Receiver Metamask ID" name="remetaid" aria-describedby="basic-addon1">
                </div>
                <br />
                <div class="input-group">
                    <span class="input-group-addon" id="basic-addon1"><span class="glyphicon glyphicon-tag"
                            aria-hidden="true"></span></span>
                    <input type="number" class="form-control" placeholder="Private key" name="privatekey" aria-describedby="basic-addon1">
                </div>
                <br />
                <div class="input-group">
                    <span class="input-group-addon" id="basic-addon1"><span class="glyphicon glyphicon-bitcoin"
                            aria-hidden="true"></span></span>
                    <input type="number" class="form-control" placeholder="Amount" name="amount" aria-describedby="basic-addon1">
                </div>
 
                <input type="submit" class="btn btn-default navbar-btn" value="Transfer" />
 
            </form>
        </div>
    </div>
</div>

<img class="img" src="/images/4dabloons.jpeg" alt="4dabloons">



<%- include('_partial/_footer'); -%>